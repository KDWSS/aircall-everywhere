{
  "name": "aircall-everywhere",
  "version": "0.1.1",
  "description": "Library to embed and communicate with aircall-phone in any webpage",
  "main": "index.js",
  "scripts": {
    "precommit": "pretty-quick --staged",
    "start": "./scripts/development.sh",
    "build": "./scripts/build.sh",
    "prepublishOnly": "./scripts/publish.sh",
    "test": "karma start --singleRun true scripts/karma.conf.js",
    "test-watch": "karma start scripts/karma.conf.js",
    "coverage": "karma start --singleRun true --reporters coverage-istanbul scripts/karma.conf.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aircall/aircall-everywhere.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/aircall/aircall-embed-sdk/issues"
  },
  "homepage": "https://github.com/aircall/aircall-embed-sdk#readme",
  "dependencies": {
    "babel-core": "6.x",
    "babel-loader": "7.x",
    "babel-plugin-istanbul": "4.x",
    "babel-plugin-rewire": "1.x",
    "babel-preset-env": "1.x",
    "gh-pages": "1.x",
    "html-webpack-plugin": "3.x",
    "husky": "0.x",
    "istanbul-instrumenter-loader": "3.x",
    "jasmine-core": "3.x",
    "karma": "2.x",
    "karma-babel-preprocessor": "7.x",
    "karma-chrome-launcher": "2.x",
    "karma-coverage": "1.x",
    "karma-coverage-istanbul-reporter": "2.x",
    "karma-jasmine": "1.x",
    "karma-sourcemap-loader": "0.x",
    "karma-webpack": "3.x",
    "pretty-quick": "1.x",
    "webpack": "4.x",
    "webpack-cli": "2.x",
    "webpack-serve": "1.x"
  }
}
